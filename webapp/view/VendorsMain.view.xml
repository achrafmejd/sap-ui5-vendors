<mvc:View
	controllerName="vendors.controller.VendorsMain"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:mc="sap.suite.ui.microchart"
	xmlns:sfb="sap.ui.comp.smartfilterbar"
	>
	 	<!-- SmartFilterBar - Start -->
        <sfb:SmartFilterBar
		    id="smartFilterBar"
		    entitySet="VENDORSHeadersSetSet"
		    basicSearchFieldName="Lifnr"
		    persistencyKey="UniqueAndStablePersistencyKey">
            <sfb:controlConfiguration>
                <sfb:ControlConfiguration id="_IDGenControlConfiguration1" key="" mandatory="mandatory" visibleInAdvancedArea="true" filterType="single" />
            </sfb:controlConfiguration>
        </sfb:SmartFilterBar>
		<!-- SmartFilterBar - End -->

		<!-- Tiles Statistics - Start -->
		<FlexBox id="_IDGenFlexBox1" class="flexbox-tiles-container" direction="Row" justifyContent="SpaceAround">
			<GenericTile id="_IDGenGenericTile1" press="press" header="This is OneByOne tile with" subheader="a RadialMicroChart" frameType="OneByOne">
				<tileContent>
					<TileContent id="_IDGenTileContent1" footer="Some footer">
						<content>
							<mc:RadialMicroChart id="_IDGenRadialMicroChart1" size="Responsive" percentage="99.9" alignContent="Right" valueColor="Neutral"/>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
			<GenericTile id="_IDGenGenericTile2" press="press" header="This is OneByOne tile with" subheader="a RadialMicroChart" frameType="OneByOne">
				<tileContent>
					<TileContent id="_IDGenTileContent22" footer="Some footer">
						<content>
							<mc:RadialMicroChart id="_IDGenRadialMicroChart2" size="Responsive" percentage="99.9" alignContent="Right" valueColor="Neutral"/>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
			<GenericTile id="_IDGenGenericTile3" press="press" header="This is OneByOne tile with" subheader="a RadialMicroChart" frameType="OneByOne">
				<tileContent>
					<TileContent id="_IDGenTileContent33" footer="Some footer">
						<content>
							<mc:RadialMicroChart id="_IDGenRadialMicroChart3" size="Responsive" percentage="99.9" alignContent="Right" valueColor="Neutral"/>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
		</FlexBox>
		<!-- Tiles Statistics - End -->

		<!-- Grid Table - Start -->
		
		<!-- Grid Table - End -->
</mvc:View>

